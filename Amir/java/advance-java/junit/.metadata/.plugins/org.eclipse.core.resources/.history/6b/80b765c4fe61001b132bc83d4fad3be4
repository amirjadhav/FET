package com.cybage;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class ArithmaticTest {

	Arithmatic arithmatic = new Arithmatic();
	@Test 		// @Test --> We are writing this method for unit testing
	void testAdd() {
		int actual  = arithmatic.add(10, 20)
		assertEquals(10, 10);		//pass
		assertEquals(10, 20);		//fail
	}

}
