package com.cybage.threading;

public class XYZProject {
	public static void main(String[] args) throws InterruptedException {
		System.out.println("welcome to XYZ project");

		Developer d = new Developer();
		Tester t = new Tester();
		Deployeer d1 = new Deployeer();
		d.start();
		t.start();
		d1.start();
		
		d.join(12000);
		t.join();
		d1.join();
		System.out.println("Peoject Completed");

	}
}
