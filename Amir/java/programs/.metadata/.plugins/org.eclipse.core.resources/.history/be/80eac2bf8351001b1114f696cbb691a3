 package com.cybage;

public class EmployeeServiceImpl implements EmployeeService {

	private Employee emp[] =new Employee[10];
	int count = 0;
	{
		emp[count] = new RegularEmployee("Regualar","pune101",123);
		count++;
		
		emp[count] = new RetiredEmployee("Regualar","pune101",123);
		count++;
	}
	@Override
	public void add() {
		System.out.println("Adding new Employee");
		
	}

	@Override
	public void display() {
		System.out.println("Display");
		
		for(Employee e : emp) {
			System.out.println(e); 
		}
		
	}

}
